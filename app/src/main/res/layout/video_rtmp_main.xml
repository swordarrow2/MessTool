<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ran="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rtmp_mainLinearLayout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <com.meng.messtool.customview.MaterialDesign.MDEditText
        android:id="@+id/rtmp_mainEditTextUrl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        ran:default_value=""
        ran:hint="rtmp服务器" />

    <com.meng.messtool.customview.MaterialDesign.MDEditText
        android:id="@+id/rtmp_mainEditTextCode"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        ran:default_value=""
        ran:hint="推流码" />

    <Button
        android:id="@+id/rtmp_mainButtonSelect"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="选择文件" />

    <Button
        android:id="@+id/rtmp_mainButtonStart"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="开始" />

</LinearLayout>

